{
  "name": "fphoto-renamer-gui-e2e",
  "private": true,
  "version": "0.1.0",
  "description": "Playwright browser UI tests for fphoto-renamer GUI",
  "type": "module",
  "scripts": {
    "test:ui": "playwright test -c playwright.config.mjs",
    "test:ui:headed": "playwright test -c playwright.config.mjs --headed",
