{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "fphoto-renamer",
  "version": "0.1.0",
  "identifier": "com.kelly.fphoto-renamer",
  "build": {
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "fphoto-renamer",
        "width": 1200,
        "height": 860,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": "all"
  }
}
